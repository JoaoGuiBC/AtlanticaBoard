// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Employee {
  id String @id @default(uuid())

  email    String @unique
  name     String
  password String

  isAdmin Boolean @default(false)

  @@map("employees")
}
